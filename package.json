{
  "name": "BatchDialerServ",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precy:run": "rm -r -d cypress\\screenshots cypress\\results cypress\\reports",
    "report-t": "npm run report:merge & npm run report:generate",
    "cy:run": "npx cypress run --browser chrome --headless --spec cypress/integration/1user.spec.js",
    "report": "npm run cy:run & npm run report:merge & npm run report:generate & npm run report:copyScreenshots",
    "report:merge": "mochawesome-merge cypress/results/json/mochawesome*.json > cypress/results/mochawesome-bundle.json",
    "report:generate": "marge --inline cypress/results/mochawesome-bundle.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress\\screenshots cypress\\reports\\html\\cypress\\results\\screenshots"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^7.0.0",
    "cypress-mochawesome-reporter": "^1.3.0",
    "cypress-multi-reporters": "^1.4.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0"
  },
  "dependencies": {
    "cypress-downloadfile": "^1.2.0",
    "cypress-file-upload": "^5.0.2",
    "cypress-promise": "^1.1.0",
    "cypress-xpath": "^1.6.2",
    "mocha": "^8.3.2",
    "mochawesome-screenshots": "^1.7.3",
    "npm-run-all": "^4.1.5",
    "pdf-parse": "^1.1.1"
  }
}
